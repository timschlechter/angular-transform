<?xml version="1.0" encoding="UTF-8"?>
<company>
    <name><at>{{company}}</at></name>
    <at ng-if="employees.length">
        <employees>
            <at ng-repeat="employee in employees | orderBy:'age'">
                <employee>
                    <name age="{{employee.age}}"><at>{{employee.name}}</at></name>
                </employee>
            </at>
        </employees>
    </at>
</company>